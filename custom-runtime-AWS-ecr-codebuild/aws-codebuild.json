        "value": "us-west-2"
      },
      {
        "name": "AWS_ACCOUNT_ID",
        "value": "268282262010"
      },
      {
        "name": "SOURCE_IMAGE_REPO",
        "value": "container.repository.cloudera.com/cloudera/dex/dex-spark-runtime-2.4.5"
